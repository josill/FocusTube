{
    "manifest_version": 3,
    "name": "FocusTube",
    "version": "0.1.0",
    "permissions": [
        "http://youtube.com",
        "https://youtube.com",
        "http://youtube.com/*",
        "https://youtube.com/*",
        "http://*.youtube.com/*",
        "https://*.youtube.com/*"
    ],
    "content_scripts": [{
        "matches": [
            "*://youtube.com/*"
        ],
        "js": ["script.js"],
        "css": ["styles.css"]
    }]
}